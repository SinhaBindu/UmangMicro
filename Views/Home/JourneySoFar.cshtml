            <div class="month-list">23</div>
            <div class="month-list">24</div>
        </div>
        <div class="train-holiday">
            <div class="holiday-fixed">
                <h3 class="text-title-1">3 Months - Training & Holidays</h3>
                <div class="holiday-liner"></div>
            </div>
            <div class="month-list">25</div>
            <div class="month-list">26</div>
            <div class="month-list">27</div>
